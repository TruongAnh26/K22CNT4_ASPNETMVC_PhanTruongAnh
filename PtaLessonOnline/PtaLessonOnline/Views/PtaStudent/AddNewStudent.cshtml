
@{
    ViewBag.Title = "AddNewStudent";
}

<h2>Thêm mới sinh viên</h2>
@using (Html.BeginForm())
{
    <div>
        @Html.Label("Họ tên")
        @Html.TextBox("fullName")
    </div>
    <div>
        @Html.Label("Mã số")
        @Html.TextBox("MaSV")
    </div>
    <div>
        @Html.Label("Tài khoản")
        @Html.TextBox("taiKhoan")
    </div>
    <div>
        @Html.Label("Mật khẩu")
        @Html.Password("passWord")
    </div>
    <button>submit</button>
            }


